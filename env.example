# LaunchDarkly Configuration
# Copy this file to .env and fill in your actual values

# Your LaunchDarkly API key (found in Account Settings > Access Tokens)
LD_API_KEY=your_api_key_here

# Your LaunchDarkly project key (found in Project Settings)
LD_PROJECT_KEY=your_project_key_here

# Your LaunchDarkly environment key (e.g., 'production', 'staging', 'development')
LD_ENVIRONMENT_KEY=your_environment_key_here

# Flask Configuration (optional)
FLASK_SECRET_KEY=your_secret_key_here
